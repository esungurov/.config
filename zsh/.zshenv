# Move zsh dotfiles 

  export ZDOTDIR=$HOME/.config/zsh

# Move Starship Config file  

  export STARSHIP_CONFIG=$HOME/.config/starship/starship.toml

# Move less history

  export LESSHISTFILE=$HOME/.config/less/history

# Move terraform config file

  export TF_CLI_CONFIG_FILE=$HOME/.config/terraform/.terraformrc

# Move vim config file

  export VIMINIT=':set runtimepath+=$HOME/.config/vim/.vim|:source $HOME/.config/vim/.vimrc'
  